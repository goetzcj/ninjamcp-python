# Include important files in the package distribution
include README.md
include LICENSE
include .env.example
recursive-include docs *.md
recursive-include src *.py
recursive-exclude * __pycache__
recursive-exclude * *.py[co]
recursive-exclude * *.so
recursive-exclude * .DS_Store
exclude .env
exclude tokens.json
exclude test_*.py
exclude *_test.py
exclude demo_*.py
exclude check_*.py
exclude quick_*.py
